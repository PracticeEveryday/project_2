  "definitions": {
    "Cocktail": {
      "properties": {
        "name": { "type": "string" },
        "ingredient": { "type": "string" },
        "imageUrl": { "type": "string" },
        "rank": { "type": "string" },
        "taste": { "type": "string" },
        "descriptoin": { "type": "string" },
        "userId": { "type": "string" }
      }
    },
    "Like": {
      "properties": {
        "giveUserId": { "type": "string" },
        "getCocktailId": { "type": "string" },
        "name": { "type": "string" }
      }
    },
    "Rank": {
      "properties": {
        "name": { "type": "string" },
        "visitors": { "type": "string" },
        "years": { "type": "string" },
        "rank": { "type": "string" }
      }
    },
    "User": {
      "properties": { "email": { "type": "string" }, "password": { "type": "string" }, "name": { "type": "string" } }
    }
  }
}
